{
  "name": "javascript-practice",
  "version": "1.0.0",
  "description": "practice javascript",
  "main": "index.js",
  "engines": {
    "node": "7.8"
  },
  "scripts": {
    "test": "xo",
    "build": "./node_modules/.bin/gulp build",
    "watch": "./node_modules/.bin/gulp watch",
    "precommit": "lint-staged"
  },
  "xo": {
    "env": [
      "node",
      "browser",
      "es6"
    ],
    "space": 2,
    "rules": {
      "operator-linebreak": [
        2,
        "before"
      ]
    },
    "ignore": [
      "src/js/loadJS.js",
      "public/**/*"
    ]
  },
  "author": "Jakob Anderson",
  "license": "MIT",
  "lint-staged": {
    "src/*": []
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-bro": "^1.0.2",
    "gulp-postcss": "^6.4.0",
    "gulp-sourcemaps": "^2.5.1",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "localforage": "^1.5.0",
    "rollup-stream": "^1.19.0",
    "static-eval": "^1.1.1",
    "uglifyify": "^3.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "xo": "^0.18.1"
  },
  "devDependencies": {
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "gulp-livereload": "^3.8.1",
    "gulp-sass": "^3.1.0",
    "gulp-svgo": "^1.0.3",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "watchify": "^3.9.0"
  }
}
